<%- include("header"); -%>
  <div class="box" id="heading">
    <h1><%= title %></h1>
  </div>

  <div class="box">
      <% items.forEach(item => { %>
        <div class="item">
          <input type="checkbox" value=""><p><%= item.name %></p>
        </div>

      <% }) %>

      <form class="item" action="/" method="post">
        <input type="text" name="item" placeholder="Add Task" autocomplete="off">
        <button type="submit" name="list" value=<%= title %>>+</button>
      </form>
  </div>
<%- include("footer"); -%>
